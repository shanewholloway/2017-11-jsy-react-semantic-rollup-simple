export default @{}
  vendor: @[]
    './app/vendor.js'

  jsy: @[]
    @{} roots: @[] './app/main.jsy'

    @{} roots: @[] './app/simple.jsy'

  files: @[]
    @{} from: './node_modules/semantic-ui-css-latolatin/dist'
        to: './dist/public/css/'
        watch: false

  opt: @{}
    common: @{}
      format: 'iife'
      viz: true, gzip: true
      outdir: './dist/public'
      globalModules: @{}
        'react': 'window.React'
        'react-dom': 'window.ReactDOM'

      /*
      aliasModules: @{}
        'react': 'preact-compat'
        'react-dom': 'preact-compat'
      */

    development: @{}
      production: false, sourcemap: true
      progress: true

    production: @{}
      production: true, sourcemap: false
      progress: @{} clearLine: false
      uglify: @{} 
        compress: @{} warnings: false
        output: @{} comments: false
        sourceMap: false

